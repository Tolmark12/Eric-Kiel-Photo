<% body_only ||= nil %>
<%= grid(@navs, :sortable => true, :filterable => true, :paginatable => true, :page => (params[:nav_page] || 1), :ajax => body_only) do |g| %>
  <% g.default_action Proc.new {|nav| %{ 	return ajaxClick('#{edit_bento_nav_path(nav)}'); } } %>
  
  <% g.add_column :name %>
<% end %>